<?xml version="1.0" encoding="utf-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <!-- Required properties -->
        <em:id>akregator_integration@mdemeyer.be</em:id>
        <em:type>2</em:type>
        <em:version>0.2</em:version>
        <em:bootstrap>true</em:bootstrap>
        <em:unpack>true</em:unpack>
        <em:name>Akregator Integration</em:name>

        <!-- Optional properties -->
        <em:description>Open web feeds with Akregator.</em:description>
        <em:creator>Maarten De Meyer</em:creator>
        <em:developer>bcooksley</em:developer>
        <em:homepageURL>http://kde-apps.org/content/show.php/?content=163096</em:homepageURL>

        <!-- Firefox -->
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>33.0</em:minVersion>
                <em:maxVersion>40.*</em:maxVersion>
            </Description>
        </em:targetApplication>

        <em:targetPlatform>Linux</em:targetPlatform>
        <em:targetPlatform>FreeBSD</em:targetPlatform>
        <em:targetPlatform>OpenBSD</em:targetPlatform>
        <em:targetPlatform>NetBSD</em:targetPlatform>

        <!-- Localization -->
        <em:localized>
            <Description>
                <em:locale>nl</em:locale>
                <em:name>Akregator Integratie</em:name>
                <em:description>Open webfeeds met Akregator.</em:description>
            </Description>
        </em:localized>

        <!-- Remove this later (read docs) -->
        <em:multiprocessCompatible>true</em:multiprocessCompatible>
    </Description>
</RDF>
